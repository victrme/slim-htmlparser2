{
	"name": "@victr/slim-htmlparser2",
	"version": "0.0.0",
	"description": "Fast & forgiving HTML/XML parser, only with Parser",
	"keywords": [
		"html",
		"parser",
		"streams",
		"xml",
		"dom",
		"rss",
		"feed",
		"atom"
	],
	"repository": {
		"type": "git",
		"url": "git://github.com/victrme/htmlparser2.git"
	},
	"license": "MIT",
	"author": "Felix Boehm <me@feedic.com>",
	"tasks": {
		"format": "deno fmt",
		"lint": "deno lint",
